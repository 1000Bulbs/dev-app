<script src="http://d3js.org/d3.v3.min.js" charset="utf-8"></script>
<script>

var results = [];

var FizzBuzz = function(num) {
  results[num - 1] = num;
  d3.json('/isFizz/' + num, function(error, json) { 
    if (json.result) {
      results[num - 1] = 'Fizz';
    }
  });
  d3.json('/isBuzz/' + num, function(error, json) {
    if (json.result) {
      results[num - 1] = 'Buzz';
    }
  });
  d3.json('/isFizzBuzz/' + num, function(error, json) {
    if (json.result) {
      results[num - 1] = 'FizzBuzz';
    }
  });
}
 
